@using TravelTripProject.Models.Sınıflar
@model List<Blog>



<div class="box1-left">
    @foreach (var item in Model)
    {

        <div class="bot-img">
            <img src="@item.BlogImage" width="170" height="80" alt="" />
        </div>
        <div class="bot-desc1">
            <p>@item.Aciklama.Substring(0, Math.Min(item.Aciklama.Length, 70))... </p>
        </div>
        <div class="clear"></div>

    }
    </div>
