@using TravelTripProject.Models.Sınıflar;
@model List<Blog>
    

<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>
    <div class="box-left">
        @foreach (var item in Model)
        {


        <div class="bot-img">
            <img src="@item.BlogImage" width="170" height="80" alt="" />
        </div>
        <div class="bot-desc">
          <p>@item.Aciklama.Substring(0, Math.Min(item.Aciklama.Length,70))</p>
          
        </div>
        <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5","Default")
</div>